<!-- views/partials/_header.ejs -->

<!DOCTYPE html>
<html lang="en">
  <head></head>
  <body>
    <% if (user) { %>
    	<p>Logged in as: <%= user %></p>
      <form action="/logout" method="POST">
      <input type="submit" value="Logout">
      </form>
    <% } else{ %>
    <a href="/login">Login</a>
    <a href="/register">Register</a>
    <% } %>
  </body>
</html>

